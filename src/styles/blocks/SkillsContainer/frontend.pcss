.skill_html {
    position: absolute;
    width: 139PX;
    height: 139PX;
    left: 5PX;
    top: 69PX;
    mix-blend-mode: normal;
    opacity: 0.1;
    border: 8PX solid #313131;
    border-radius: 100%;
}
.skill_html_title {
    position: absolute;
    width: 62PX;
    height: 25PX;
    left: 46PX;
    top: 129PX;
    font-family: Open Sans;
    font-style: normal;
    font-weight: bold;
    font-size: 18PX;
    line-height: 25PX;
    display: flex;
    align-items: center;
    color: rgba(16, 0, 34, 0.5);
    z-index: 3;
}
.skill_html_ellipse {
    position: absolute;
    height: 77PX;
    width: 77PX;
    left: -2PX;
    top: 63PX;
    background: svg-load(
        "htmlEllipse.svg",
        fill=rgba(255, 255, 255, 1),
        width=77PX,
        height=77PX
    );
}

