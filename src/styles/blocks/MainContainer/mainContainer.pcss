    top: 883PX;
}
.text {
    font-family: Open Sans;
    font-style: normal;
    font-weight: 600;
    font-size: 18PX;
    line-height: 25PX;
    display: flex;
    align-items: center;

    color: #ffffff;
}
.scroll_arrows {
    height: 13.41PX;
    width: 13.12PX;
    border-radius: 0PX;
    background: svg-load(
        "scroll.svg",
        fill= "#FFFFFF",
        width= 13.12PX,
        height= 13.41PX
    );
}