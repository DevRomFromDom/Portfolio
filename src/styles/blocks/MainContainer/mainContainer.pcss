.name {
    position: absolute;
    width: 427PX;
    height: 120PX;
    left: 533PX;
    top: 425PX;

    /* Desktop/h1 */

    font-family: Open Sans;
    font-style: normal;
    font-weight: 800;
    font-size: 72PX;
