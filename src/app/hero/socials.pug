.header__buttons 
    ul.socials 
        - var socials = [
        - {name:"insta", width:"25",height:"25", href:"#"},
        - {name:"vk", width:"32",height:"32", href:"#"},
        - {name:"github", width:"25",height:"25", href:"#"},
        - {name:"envelope", width:"29",height:"29", href:"#"},
        - ]
        each item in socials 
            li.socials__item 
                a(href=item.href).socials__link
                    +icon(item, 'socials__icon')